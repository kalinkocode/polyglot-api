{
    "name": "kalinkocode/polyglot-api",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Lamine Diallo",
            "email": "mldiallo@kalinkotech.com",
            "homepage": "https://github.com/kalinkocode/polyglot-api"
        }
    ],
    "homepage": "https://github.com/kalinkocode/polyglot-api",
    "keywords": ["Laravel", "PolyglotApi"],
    "require": {
        "illuminate/support": "~10"
    },
    "require-dev": {
        "phpunit/phpunit": "~10",
        "orchestra/testbench": "~8",
        "laravel/pint": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Kalinkocode\\PolyglotApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kalinkocode\\PolyglotApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": [],
        "post-update-cmd": [],
        "post-root-package-install": [],
        "post-create-project-cmd": [],
        "format": "./vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kalinkocode\\PolyglotApi\\PolyglotApiServiceProvider"
            ],
            "aliases": {
                "PolyglotApi": "Kalinkocode\\PolyglotApi\\Facades\\PolyglotApi"
            }
        }
    }
}
